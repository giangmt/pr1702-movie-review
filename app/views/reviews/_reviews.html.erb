<% @movie.reviews.order("created_at DESC").each do |review| %>
  <div id="more" class="review-properties" >
    <p><%= t ".review_by" %>: <%= review.user.email %>
      | <%= review.created_at %></p>
    <h4><%= review.title %></h4>
    <p><%= review.content %></p>
  </div>
  <div class="line"></div>
<% end %>
